{
  "name": "nile-collective-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf node_modules/.vite",
    "kill-port": "for port in 3000 3001 3002 3003 3004 3005; do lsof -ti:$port | xargs kill -9 2>/dev/null || true; done",
    "predev": "npm run clean && npm run kill-port",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "canvas-confetti": "^1.9.4",
    "framer-motion": "^12.26.2",
    "lucide-react": "^0.562.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-paystack": "^3.0.5",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.8"
  }
}
